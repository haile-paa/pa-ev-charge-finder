{
  "expo": {
    "name": "EV Charge Finder",
    "slug": "ev-charge-finder",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/Y.png",
    "scheme": "EV Plug",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Y.png",
        "backgroundColor": "#0D1C1F"
      },
      "edgeToEdgeEnabled": true,
      "package": "PA.Ev",
      "usesCleartextTraffic": true,
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "42aa105f-ab07-4d5e-bb5e-465e11f49d45"
      }
    }
  }
}
